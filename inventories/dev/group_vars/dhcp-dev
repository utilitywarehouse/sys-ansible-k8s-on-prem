  dhcp-host=6C:4B:90:6A:71:40,10.88.0.34  # etcd-2
  dhcp-host=94:18:82:88:bc:30,10.88.0.64  # master-0
  dhcp-host=94:18:82:80:87:78,10.88.0.65  # master-1
  dhcp-host=60:45:cb:9d:a9:13,10.88.0.128 # worker-0
  dhcp-host=60:45:cb:9d:a7:12,10.88.0.129 # worker-1
  dhcp-host=60:45:cb:9b:bf:8c,10.88.0.130 # worker-2
  dhcp-host=94:C6:91:86:D3:B1,10.88.0.131 # worker-3
